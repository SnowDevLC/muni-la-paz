{
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
